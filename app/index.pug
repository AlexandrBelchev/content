doctype html
html(lang='en')
    head
        meta(charset='UTF-8')
        meta(name='viewport' content='initial-scale=1.0, width=device-width')
        meta(http-equiv='X-UA-Compatible' content='ie=edge')
        meta(name='description' content='Roran psd template by LunarTemplates. Lorem ipsum dolor sit amet, consectetur adipiscing elit.\
      vulputate, vehicula nisi vitae, lobortis ante.')
        meta(name='msapplication-config' content='browserconfig.xml')
        meta(name='title' content='Roran template')
        meta(name='keywords' content='Roran, ronan, rorran')
        meta(name='og:title' content='Roran template')
        meta(name='og:description' content='Roran psd template by LunarTemplates. Lorem ipsum dolor sit amet, consectetur adipiscing elit.\
      vulputate, vehicula nisi vitae, lobortis ante.')
        link(rel='icon' type='image/png' sizes='16x16' href='img/favicon-16x16.png')
        link(rel='apple-touch-icon' sizes='180x180' href='img/apple-icon-180x180.png')
        link(rel='stylesheet' href='css/main.css')
        link(href='https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i' rel='stylesheet')
        link(href='https://use.fontawesome.com/releases/v5.0.6/css/all.css' rel='stylesheet')
        script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js')
        title Roran template
    body
        header
            .container
                .header__content
                    .header__content-left-part
                        h1.header__title Roran
                        span.header__second-title psd template
                        span.header__third-title By LunarTemplates
                        span.header__link www.lunartemplates.com
                    .header__content-right-part
                        span.header__text
                            | Free to download
                            br
                            | at lunartemplates.com
                .main-arrow
                    a(href='.about')
                        img(src='img/arrow3.png' alt='')
        section#main__menu.main__menu
            .container
                nav
                    .logo
                        a.logo__text(href='') Roran
                    a#menu__hamburger(href='#main__menu')
                        span
                        span
                        span
                    ul#menu.menu
                        - let menus = [{link: 'header', text: 'Home'},{link: '.about', text: 'About'},{link: '.team', text: 'Team'},{link: '.services', text: 'Services'},{link: '.portfolio', text: 'Portfolio'},{link: '.contact', text: 'Contact'}]
                        each menu, i in menus
                            li
                                a(href=menu.link)=menu.text
                    .social-links
                        ul
                            li
                                a(href='')
                                    i.fab.fa-linkedin-in
                            li
                                a(href='')
                                    i.fab.fa-pinterest-p
                            li
                                a(href='')
                                    i.fab.fa-twitter
                            li
                                a(href='')
                                    i.fab.fa-facebook-f
        section.about
            .about__lorem
                .container
                    .about__heading
                        h2 About Roran
                    .about__heading-2
                        span Creative Studio
                    .about__description
                        p
                            | Lorem ipsum dolor sit amet, consectetur adipiscing elit.
                            br
                            | vulputate, vehicula nisi vitae, lobortis ante. Praesent sodales iaculis ultricies
                    .lorem
                        - let lorem_desc="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer at purus vulputate, vehicula nisi vitae, lobortis ante."
                        - let lorems = [{lorem__heading: 'Some fancy ', lorem_span: 'colors'},{lorem__heading: 'Enough ', lorem_span: 'coffee breaks'},{lorem__heading: 'A little bit of ', lorem_span: 'magic'}]
                        each lorem, i in lorems
                            .lorem__item
                                .lorem-icon
                                    img(src="img/lorem-icon" + i + ".png" alt='')
                                p.lorem__heading=lorem.lorem__heading
                                    span=lorem.lorem_span
                                p.lorem__description #{lorem_desc}
            .numbers
                .container
                    - let socials = [{span: '1085', p: 'Empty cups'},{span: '102', p: 'Happy clients'},{span: '25', p: 'BBQ meetings'},{span: '29', p: 'Cocktail parties'},]
                    .container
                        each social, i in socials
                            .number__item
                                .number__img
                                    img(src="img/number-icon" + i + ".png" alt='')
                                .number__description
                                    span=social.span
                                    p=social.p
            .clients
                .container
                    .clients__heading
                        h2.heading Some of our clients
                    .clients__description
                        span.clients__heart We
                        span every single one
                    .our__clients
                        -for(let i = 1; i < 5; i++ )
                            .client__item
                                img(src="img/clients-icon" + i + ".png" alt='')
        section.services
            - let lorem_ipsum = "Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer at purus vulputate, vehicula nisi vitae, lobortis ante."
            - let services_item_heading="Dictum tincidunt"
            - let services_item_2="diam"
            .container
                .services__heading
                    h2.heading Our Services
                .services__second-heading
                    span With Passion &amp; Experience
                .services-content
                    -for(let i = 1; i < 7; i++ )
                        .services__item
                            .services__img
                                img(src="img/services-icon"+i+".png" alt='')
                            .services__text
                                p.services__item-heading #{services_item_heading}
                                    span  #{services_item_2}
                                p.services__description #{lorem_ipsum}
        section.quote
            .container
                blockquote
                    | Lorem ipsum dolor sit amet,
                    b  consectetur adipiscing
                    |  elit. Integer at purus vulputate,
                    b  vehicula
                    |  nisi vitae, lobortis ante. Praesent sodales iaculis ultricies. Vestibulum et
                    b  tristique
                    |  nisl, sed volutpat ante.
                .quote__author
                    span Jonathan Doe
        section.team
            .team__top-content
                .container
                    .team__heading
                        h2.heading Meet...the team
                    .team__second-heading
                        span They are really nice!
            .team-content
                .container
                    .nav-prev
                        i.fas.fa-angle-left
                    .nav-next
                        i.fas.fa-angle-right
                    .nav-indicators
                        ul
                            li
                            li.nav-active
                            li
                            li
                            li
                    mixin social
                        li
                            a(href='')
                                i.fab.fa-twitter
                        li
                            a(href='')
                                i.fab.fa-facebook-f
                    - let names = [{team__name: 'Jonathan Doe ', team__position: 'Creative Director'},{team__name: 'Jaden Doe ', team__position: 'Mastermind'},{team__name: 'Mister Doe ', team__position: 'Designer'}]
                        each name, i in names
                            .team__item
                                .team__item-overlay
                                    .team__item-description
                                        span.team__name=name.team__name
                                        span.team__position=name.team__position
                                    .social-links
                                        ul
                                            +social
        section.portfolio
            .container
                .portfolio__top-content
                    .portfolio__heading
                        h2.heading Portfolio
                    .portfolio__second-heading
                        span This is what we&rsquo;ve been working on
                .portfolio__tabs
                    .button-group
                        each val in ['Web', 'Digital Art', 'Illustrations', 'Branding', 'All']
                            button= val
            .portfolio-content__part1
                -for(let i = 0; i < 4; i++ )
                    .portfolio__item
            .portfolio-content__part2
                .portfolio__item
                .portfolio__item.portfolio-clip
                    .portfolio__item-description
                        img(src='img/portfolio-icon1.png' alt='')
                        span Free Website PSD
                        p Web
                .portfolio__item
                .portfolio__item
        section.contact
            .container
                .contact__top-content
                    .contact__heading
                        h2.heading Contact
                    .contact__second-heading
                        span We would really love to hear from you!
            .contact__content
                .container__text
                    .contact__text-name
                        span Roran
            .contact__bottom-content
                .container.container-form
                    .contact__form
                        .contact__form-left-part
                            form#form(action='POST')
                                .form-group
                                    .contact-label
                                        label(for='first-name') Your Name
                                        span.contact-required required
                                    input#first-name(type='text' name="first-name")
                                .form-group
                                    .contact-label
                                        label(for='email') Your email
                                        span.contact-required required
                                    input#email(type='email' name="email")
                                .form-group
                                    .contact-label
                                        label(for='subject') Subject
                                    input#subject(type='text' name="subject")
                                .form-group
                                    .contact-label
                                        label(for='message') Message
                                        span.contact-required required
                                    textarea#message(rows='6' name="message")
                                button.btn(type='submit') Send
                        .contact__form-right-part
                            .contact__info
                                span.contact__info-heading Contactinfo
                                .contact__info-text
                                    p Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer at purus vulputate.
                                    p Vehicula nisi vitae, lobortis ante. Praesent sodales iaculis ultricies.
                                .contact__info-info
                                    .contact__info-address.contact__info-contacts
                                        .contact__icon
                                            img(src='img/contact-icon1.png' alt='')
                                        .contact__info-text2
                                            p Rue de Halle 245
                                            p 1000 Brussels
                                    .contact__info-telephone.contact__info-contacts
                                        .contact__icon
                                            img(src='img/contact-icon2.png' alt='')
                                        .contact__info-text2
                                            p 03 123 45 67
                                            p 0412 345 678
                                    .contact__info-email.contact__info-contacts
                                        .contact__icon
                                            img(src='img/contact-icon3.png' alt='')
                                        .contact__info-text2
                                            p info@roran.com
                                            p question@roran.com
        footer
            .container
                .footer__copyright
                    p &copy; 2014 Roran. All rights reserved
                .footer-arrow
                    a(href='header')
                        i.fas.fa-angle-up
                .footer__social-links.social-links
                    ul
                        li
                            a(href='')
                                i.fab.fa-linkedin-in
                        li
                            a(href='')
                                i.fab.fa-pinterest-p
                        li
                            a(href='')
                                i.fab.fa-twitter
                        li
                            a(href='')
                                i.fab.fa-facebook-f
        script(src='js/main.js')